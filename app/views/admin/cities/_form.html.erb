<div class="row">
  <div class="col-md-4">
    <%= form_for [:admin, @city] do |f| %>
      <%= render "shared/error_messages", object: @city %>

      <%= f.label :name %>
      <%= f.text_field :name, class: "form-control" %>

      <%= f.submit yield(:submit), class: "btn btn-success" %>
    <% end %>    
  </div>
</div>
